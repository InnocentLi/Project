var s;

function de(){
	//  $("#song").remove();
}
function a(){
    window.s = document.getElementById('find');
   console.log(document.getElementById('find').value);
          		
 // console.log('find.value');
    var value = s.value;
        $.ajax({
        	url: 'https://api.douban.com/v2/music/search?q='+value,
        	type:  'GET',
        	dataType: 'jsonp',
        	success: function (data) {
        		//console.log(data);
        		createHT(data);

               	}
       })
           
  //createHT(data);

    function createHT(p){
    	   console.log(p);
          //   console.log(p.musics.lenth);
             for(var i = 0;i<p.musics.length;i++){  
               var j =0; j++;         
                 // console.log(p.musics[i].image);
                 // console.log(p.musics[i].title);
                 	b(p.musics[i].image,p.musics[i].title,j+1)
                }
             }

       }
          function b(image,title,num){
          
              console.log(image);
              console.log(title); 
              console.log(num);
             $('#song').after('<img>');
             c(image,title,num);
   }   
          function c(image,title,num){
               $('img:eq('+num+')').attr('src', image).attr('value', title);
   }
               
$('#find').keydown(function(event){de();a();});


</script>
</body>
</html> 
